# Gemini API Key
GEMINI_API_KEY="YOUR_GEMINI_API_KEY"

# Paperless-NGX API
PAPERLESS_URL="http://localhost:8001"
PAPERLESS_API_TOKEN="YOUR_PAPERLESS_API_TOKEN"

# Backup Configuration
BACKUP_PROVIDER="GCS" # Options: "GCS" (Google Cloud Storage) or "S3" (AWS S3)
BACKUP_BUCKET_NAME="your-backup-bucket-name"

# Google Cloud Storage Configuration (if using GCS)
# The path to your Google Cloud service account JSON key file
# This file contains credentials for authentication with GCS.
GCS_SERVICE_ACCOUNT_FILE="/path/to/your/gcs-service-account.json"

# AWS S3 Configuration (if using S3 - currently commented out)
# AWS_ACCESS_KEY_ID="YOUR_AWS_ACCESS_KEY_ID"
# AWS_SECRET_ACCESS_KEY="YOUR_AWS_SECRET_ACCESS_KEY"
# AWS_REGION="your-s3-bucket-region"
